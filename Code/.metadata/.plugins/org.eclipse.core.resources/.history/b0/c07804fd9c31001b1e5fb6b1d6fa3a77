package miniSGBD;

public class Rid {

	private PageId pageId;
	private int slotIdx;
	
	public Rid(PageId pageIdx,int slotIdx) {
		this.pageId = pageIdx;
		this.setSlotIdx(slotIdx);
	}

	public int getSlotIdx() {
		return slotIdx;
	}

	public void setSlotIdx(int slotIdx) {
		this.slotIdx = slotIdx;
	}
}
