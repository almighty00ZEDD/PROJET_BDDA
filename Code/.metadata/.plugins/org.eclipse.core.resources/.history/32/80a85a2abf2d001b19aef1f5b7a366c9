package miniSGBD;

public class DBInfoTests {

	public void TestsSurTP4() {
		DBInfo DBI = DBInfo.getInstance();
		DBI.Init();
		RelationInfo r1 = new RelationInfo(),r2 = new RelationInfo(),r3 = new RelationInfo(),r4 = new RelationInfo();
		DBI.AddRelation(r1);
		DBI.AddRelation(r2);
		DBI.AddRelation(r3);
		DBI.AddRelation(r4);
		DBI.Finish();
		DBI.Init();
	}
}
